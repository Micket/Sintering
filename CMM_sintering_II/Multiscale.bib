pages = {371--392},
title = {{Adaptive Bridging of Scales in Continuum Modeling Based on Error Control}},
url = {http://www.begellhouse.com/journals/61fd1b191cf7e96f,6a4cc29505086840,7cf86741344095e8.html},
volume = {6},
